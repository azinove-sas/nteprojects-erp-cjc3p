{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@libraries/*": ["libraries/*"],
      "@config/*": ["config/*"],
      "@views/*": ["views/*"],
      "@models/*": ["models/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constant/*": ["constant/*"],
      "@utils/*": ["utils/*"]
    }
  },
  "extends": "tsconfig/nextjs.json",
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
